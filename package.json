{
  "name": "yvi-soft-fullstack",
  "version": "1.0.0",
  "description": "YVI Soft website with separated frontend and backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "cd backend && npm start",
    "dev": "node start-dev.js",
    "build": "cd backend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "deploy:backend": "node deploy-backend.js",
    "deploy:frontend": "node deploy-frontend.js",
    "deploy": "npm run build:frontend && npm run deploy:frontend",
    "deploy:full": "node deploy-full.js",
    "test:connection": "node test-frontend-backend.js"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "keywords": [
    "react",
    "nodejs",
    "fullstack",
    "website"
  ],
  "author": "YVI Soft Solutions",
  "license": "MIT"
}